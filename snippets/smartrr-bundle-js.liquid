      if (bundleConfig.chosenPack !== "" &&
          Pack.HasPack(handler.bundle.packs, atob(bundleConfig.chosenPack))) {
        var chosenPack = Pack.GetPack(handler.bundle.packs, atob(bundleConfig.chosenPack));
        if (chosenPack.variantAvailabe) {
