      function FilterProducts() {
        var tags = [];
        $filterTags.forEach(function ($tag) {
          if (sh.containsClass($tag, ct.ACTIVE)) {
            tags.push(
              atob(
                sh.getAttribute(
                  $tag,
                  qt.PRODUCTSELECTION.HEADER.FILTER.TAGSLIST.INDIVIDUALTAG
