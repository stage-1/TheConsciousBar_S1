      var $packlist = sh.qsad($packContainer, qt.PACKSELECTION.PACKTAG);
      $packlist.forEach(function ($pack) {
        var $packName = sh.qsd($pack, qt.PACKSELECTION.PACK.NAMETAG);
        var $packDescription = sh.qsd(
          $pack,
